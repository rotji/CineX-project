{
  "name": "CineX-project-tests",
  "version": "1.0.0",
  "description": "Run unit tests on this project.",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:report": "vitest run -- --coverage --costs",
    "test:watch": "vitest",
    "prepare-tests": "node scripts/move-and-create-tests.js --move --create-stubs",
    "rv-all": "npm run prepare-tests && node scripts/run-rv-for-all.js",
    "rv-contract": "npm run prepare-tests && npx rv ."
  },
  "bin": {
    "run": "./node_modules/@stacks/rendezvous/dist/cli.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hirosystems/clarinet-sdk": "^2.3.2",
    "@stacks/connect": "^8.2.0",
    "@stacks/network": "^7.2.0",
    "@stacks/rendezvous": "^0.10.0",
    "@stacks/transactions": "^6.17.0",
    "chokidar-cli": "^3.0.0",
    "react-router-dom": "^7.8.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vitest-environment-clarinet": "^2.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.1"
  }
}
